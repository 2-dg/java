<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="이름 :" />
            <TextField fx:id="txtName" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="학년 :" />
            <TextField fx:id="txtYear" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="   반 :" />
            <TextField fx:id="txtBan" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="성별 :" />
            <TextField fx:id="txtGender" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="국어 :" />
            <TextField fx:id="txtKorean" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="영어 :" />
            <TextField fx:id="txtEnglish" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="수학 :" />
            <TextField fx:id="txtMath" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="과학 :" />
            <TextField fx:id="txtSic" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="사회 :" />
            <TextField fx:id="txtSoc" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="음악 :" />
            <TextField fx:id="txtMusic" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="총점 :" />
            <TextField fx:id="txtTotal" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label prefWidth="50.0" text="평균 :" />
            <TextField fx:id="txtAvg" prefWidth="100.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0" spacing="20.0">
         <children>
            <Button id="btnFormCal" mnemonicParsing="false" text="계산" />
            <Button id="btnFormAdd" fx:id="btnFormAdd" mnemonicParsing="false" text="저장" />
            <Button id="btnFormCancel" fx:id="btnFormCancel" mnemonicParsing="false" text="취소" />
         </children>
      </HBox>
   </children>
</VBox>